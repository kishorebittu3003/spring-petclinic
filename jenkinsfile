{
    agent 
    { 
        label 'UBUNTU_2'
    }
    stages 
    {
        stage ('git clone')
        { 
            steps
            {
            git url: https://github.com/kishorebittu3003/spring-petclinic.
           }
        }
        {
         stage ('package')
        {
            steps 
            {
                script
                {
                    sh 'mvn package'
                }
                
            }

        }

        }          
    }  
}